N = int(input("Введіть ціле число N (1<N<9): "))
while N <= 1 or N >= 9:
    print("Число N має бути в межах 1 < N < 9.")
    N = int(input("Введіть ціле число N (1<N<9): "))

# Введення символу для побудови рисунку
symbol = input("Введіть символ для побудови рисунку (наприклад, *, #, або цифру): ")

# Побудова рисунку
for i in range(1, N + 1):
    for j in range(1, i + 1):
        print(symbol, end=" ")
    print()
