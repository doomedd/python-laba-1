a = int(input("Введіть a: "))
while a <= 0:
    print("Число a має бути додатним.")
    a = int(input("Введіть a: "))

b = int(input("Введіть b: "))
while b <= 0:
    print("Число b має бути додатним.")
    b = int(input("Введіть b: "))

# Обчислення X
if a < b:
    X = a / b + 5
elif a == b:
    X = -5
else:
    X = (a * a - b) / b

# Виведення результату
print("Результат обчислення X:", X)

